{
  "name": "@nanostores/angular",
  "version": "0.0.0",
  "description": "Angular integration for Nano Stores, a tiny state manager with many atomic tree-shakable stores",
  "license": "MIT",
  "publishConfig": {
    "directory": "package"
  },
  "scripts": {
    "prepublishOnly": "rm -rf ./package && clean-publish",
    "postpublish": "rm -rf ./package"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "package"
  },
  "keywords": [
    "store",
    "state",
    "state manager",
    "nanostores",
    "ng nanostores",
    "angular"
  ],
  "peerDependencies": {
    "@angular/common": "^12.2.17",
    "@angular/core": "^12.2.17",
    "nanostores": "^0.7.1"
  },
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "clean-publish": "^4.1.1"
  }
}
